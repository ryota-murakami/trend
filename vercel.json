{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",

  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_VAPID_PUBLIC_KEY": "@next_public_vapid_public_key",
    "VAPID_PRIVATE_KEY": "@vapid_private_key",
    "GITHUB_TOKEN": "@github_token"
  }
}
